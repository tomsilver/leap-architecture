(pickup fork1 kitchentop)
(open fridge)
(getout egg1 fridge)
(close fridge)
(crackegg egg1 bigbowl)
(beat egg1 fork1 bigbowl)
(pickup bigbowl kitchentop)
(transfer egg1 bigbowl frypan)
(open dcup2)
(getout oilbottle dcup2)
(close dcup2)
(transfer oil oilbottle frypan)
(open dcup1)
(getout frypan dcup1)
(close dcup1)
(putdown frypan burner)
(switchon burner)
(fry egg1 frypan burner)
; cost = 18 (unit cost)
